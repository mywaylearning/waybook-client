{
  "name": "waybook-client",
  "version": "1.0.0",
  "description": "Waybook: An Ionic project",
  "browser": {
    "ionic": "./www/lib/ionic/js/ionic.min.js",
    "ionic-angular": "./www/lib/ionic/js/ionic-angular.min.js",
    "angular": "./www/lib/angular/angular.min.js",
    "angular-animate": "./www/lib/angular-animate/angular-animate.min.js",
    "angular-sanitize": "./www/lib/angular-sanitize/angular-sanitize.min.js",
    "angular-messages": "./www/lib/angular-messages/angular-messages.min.js",
    "angular-ui-router": "./www/lib/angular-ui-router/release/angular-ui-router.min.js",
    "restangular": "./www/lib/restangular/dist/restangular.min.js",
    "lodash": "./www/app/components/lodash.custom.min.js",
    "moment": "./node_modules/angular-moment/node_modules/moment/min/moment.min.js",
    "angular-moment": "./node_modules/angular-moment/angular-moment.min.js",
    "store": "./node_modules/store/store.min.js",
    "angular-gravatar": "./www/lib/angular-gravatar/build/angular-gravatar.min.js",
    "ionic-ion-showWhen": "./www/lib/ionic-ion-showWhen/index.js",
    "ion-wizard": "./www/lib/ion-wizard/index.js",
    "ng-tags-input": "./www/lib/ng-tags-input/ng-tags-input.min.js",
    "mentio": "./www/lib/ment.io/dist/mentio.min.js",
    "intl-phone-number": "./www/lib/international-phone-number/releases/international-phone-number.min.js",
    "state-nav-back-button": "./www/app/core/state-nav-back-button/state-nav-back-button.js",
    "ionic-datepicker": "./www/lib/ionic-datepicker/dist/ionic-datepicker.bundle.min.js",
    "highcharts": "./www/lib/highcharts/highcharts.js",
    "highcharts-more": "./www/lib/highcharts/highcharts-more.js",
    "highcharts-solidgauge": "./www/lib/highcharts/modules/solid-gauge.js",
    "highcharts-ng": "./www/lib/highcharts-ng/dist/highcharts-ng.min.js",
    "hello": "./www/lib/hello/dist/hello.all.min.js",
    "ng-segmentio": "./www/lib/ng-segmentio/build/ng-segmentio.min.js"
  },
  "browserify-shim": {
    "angular": "angular",
    "angular-sanitize": {
      "depends": "angular"
    },
    "lodash": "_",
    "moment": "moment",
    "restangular": {
      "depends": "lodash:_"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "browserify-ngannotate",
      [
        "localenvify",
        {
          "envfile": ".env.local"
        }
      ],
      [
        "localenvify",
        {
          "envfile": ".env"
        }
      ],
      [
        "uglifyify",
        {
          "global": false,
          "mangle": false,
          "compress": {
            "sequences": true,
            "dead_code": true,
            "angular": true,
            "screw_ie8": true,
            "join_vars": true,
            "unused": true
          },
          "output": {
            "comments": false
          },
          "ignore": [
            "**/www/lib/angular*",
            "**/www/lib/ionic/*",
            "**/www/lib/restangular/*",
            "**/node_modules/angular-moment*",
            "**/node_modules/store*",
            "./www/app/components/lodash.custom.min.js"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "angular-moment": "^0.10.1",
    "debug": "^2.2.0",
    "store": "^1.3.17",
    "uglify-js": "^2.5.0",

    "restangular": "^1.5.1"
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "bower": "^1.4.1",
    "browserify-ngannotate": "^1.0.1",
    "browserify-shim": "^3.8.11",
    "chalk": "^1.1.0",
    "exorcist": "^0.4.0",
    "ionic": "^1.6.1",
    "js-beautify": "^1.5.6",
    "jsonfile": "^2.2.1",
    "localenvify": "^1.0.1",
    "lodash-cli": "^3.9.1",
    "ng-annotate": "^1.0.1",
    "node-sass": "^3.1.1",
    "postcss-cli": "^1.3.1",
    "shelljs": "^0.5.0",
    "uglifyify": "^3.0.1",
    "watch": "^0.16.0",
    "watchify": "^3.2.3",

    "debowerify": "^1.3.1",
    "del": "^0.1.3",
    "babel-core": "^5.8.25",
    "browser-sync": "^2.9.12",
    "browserify": "^12.0.0",
    "browserify-istanbul": "^0.2.1",
    "browserify-ngannotate": "^1.0.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-angular": "^0.13.0",
    "gulp": "^3.8.8",
    "gulp-angular-templatecache": "^1.3.0",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-changed": "^1.0.0",
    "gulp-cssimport": "^2.1.1",
    "gulp-eslint": "^1.0.0",
    "gulp-gzip": "^0.0.8",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^1.1.0",
    "gulp-notify": "^2.0.0",
    "gulp-protractor": "^1.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-rev-all": "^0.8.22",
    "gulp-rev-replace": "^0.4.2",
    "gulp-sass": "^2.0.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.4.2",
    "gulp-util": "^3.0.1",
    "imagemin-pngcrush": "^0.1.0",
    "isparta": "^3.0.3",
    "karma": "^0.13.14",
    "karma-browserify": "^4.4.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "douglasduteil/karma-coverage#next",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "^0.3.6",
    "karma-source-map-support": "^1.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^0.1.1",
    "run-sequence": "^0.3.6"




  },
  "scripts": {
    "-deploy-": "",
    "deploy:dev": "aws s3 sync www/ s3://uidev.way.me --delete --acl public-read --profile way",
    "deploy:staging": "aws s3 sync www/ s3://uidev.way.me --delete --acl public-read --region us-west-2",
    "-js-": "",
    "build:lodash": "grep -o -C 0 '_\\.[^(]*' www/lib/restangular/dist/restangular.js | sort -u | sed -e 's/_\\.//' | paste -d, -s - | xargs -I\"{}\" -t lodash -s -o www/app/components/lodash.custom.min.js -p include={}",
    "build:browserify": "NODE_ENV=production browserify www/app/app.js | uglifyjs --compress > www/app/waybook-min.js",
    "build-js:dev": "npm run build:lodash",
    "build-js": "npm run build:lodash && npm run build:browserify",
    "-css-": "",
    "build:sass:dev": "node-sass --precision 10 --linefeed cr --source-map www/css/ scss/ --output www/css/",
    "build:sass": "node-sass --precision 10 --linefeed cr --output-style compressed scss/ --output www/css/",
    "build:autoprefix": "autoprefixer www/css/waybook.css",
    "build-styles:dev": "npm run build:sass:dev && npm run build:autoprefix",
    "build-styles": "npm run build:sass",
    "-dev-": "",
    "localdev-setup": "ln -vf .env node_modules/waybook-api/.env && cd node_modules/waybook-api && npm run localdev-setup",
    "test": "npm run build-styles:dev && npm run build-js",
    "-watch-": "",
    "watch:package-json": "",
    "watch:api-spec": "",
    "watch:browserify": "npm run build-js:dev && watchify -o www/app/waybook-min.js -v -d www/app/app.js",
    "watch:styles": "watch 'npm run build:sass:dev' scss/",
    "-runtime-": "",
    "preflight": "./scripts/flight-check",
    "serve": "ionic serve --nogulp",
    "start": "npm run watch:styles & npm run watch:browserify & ionic serve --nogulp",
    "gulp": "gulp dev"
  }
}
